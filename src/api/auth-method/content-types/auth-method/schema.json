{
  "kind": "collectionType",
  "collectionName": "auth_methods",
  "info": {
    "singularName": "auth-method",
    "pluralName": "auth-methods",
    "displayName": "Auth Method",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "identifier": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Native",
        "External",
        "Biometric"
      ]
    },
    "strategy": {
      "type": "enumeration",
      "enum": [
        "Native",
        "Google",
        "Apple",
        "Facebook"
      ]
    },
    "password": {
      "type": "string"
    },
    "passToken": {
      "type": "string",
      "unique": true
    },
    "identifierToken": {
      "type": "string",
      "unique": true
    },
    "verifyToken": {
      "type": "string",
      "unique": true
    },
    "deletedAt": {
      "type": "datetime"
    },
    "userApp": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::user-app.user-app",
      "inversedBy": "authMethods"
    }
  }
}
